import java.util.*;
public class hi{
    public static void main(String[]args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        while(n-->0){
            int m=sc.nextInt();
            if(m%3!=0){
                System.out.println("First");
            }
            else{
                System.out.println("Second");
            }
        }
    }  
}
